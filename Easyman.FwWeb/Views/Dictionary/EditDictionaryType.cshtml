@using Easyman.Common.Mvc.Controls
@model Easyman.Dto.DictionaryTypeModel
@{
    Layout = "~/Views/Shared/_CommonEdite.cshtml";
}
@section Styles{
   
}

    @Html.HiddenFor(h => h.Id)

        <div class="tab-content" style="padding-top:12px;">
            <div role="tabpanel" class="tab-pane active" id="navMenu" style="margin-bottom:50px">
              
                <div class="form-group">
                    <label for="Name" class="col-xs-4 control-label" style="text-align:right">字典类型</label>
                    <div class="col-xs-6">
                        @*@Html.TextBoxFor(f => f.Name, new { @class = "form-control", placeholder = "字典类型名", @Required = "Required" })*@
                        @Html.TextBoxFor(f => f.Name, new { @class = "form-control", placeholder = "字典类型", @Required = "Required", @oninvalid = "setCustomValidity('请输入字典类型名称')", @oninput = "setCustomValidity('')" })<i class="iRequired">*</i>
                    </div>
                </div>
                <div class="form-group">
                    <label for="Remark" class="col-xs-4 control-label" style="text-align:right">描述说明</label>
                    <div class="col-xs-6">
                        @Html.TextAreaFor(f => f.Remark, new { @class = "form-control", placeholder = "描述说明" })
                    </div>
                </div>
            </div>
        </div>
 
@section scripts{
  @*放页面自定义js*@
<script type="text/javascript">
    //母版页调用提交方法
    function SubmitForm() {
        //from表单提交路径
        var postUrl = "../api/services/api/DictionaryType/InsertOrUpdateDictionaryType";
        //提交方法-JS里面
        handleSubmitForm(postUrl);
    }
</script>
}